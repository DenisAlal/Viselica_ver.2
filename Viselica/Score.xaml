<Window x:Class="Viselica.Score"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Viselica"
        mc:Ignorable="d"
        Title="Результаты игр:" Height="409" Width="250" Icon="/Score.png" ResizeMode="NoResize" WindowStartupLocation="CenterScreen">
    <Grid Margin="0,0,0,2">
       
            <ListBox x:Name="listBox1" d:ItemsSource="{d:SampleData ItemCount=5}" Margin="9,9,9,9">
                <ListBox.Resources>
                    <Style TargetType="Border">
                        <Setter Property="CornerRadius" Value="6"/>
                    </Style>
                </ListBox.Resources>
            </ListBox>
        
    </Grid>
</Window>
